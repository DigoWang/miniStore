<view><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block wx:for="{{orderbyList}}" wx:for-item="target" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{['target',target.selected?'on':'']}}" bindtap="__e">{{''+target.text+''}}<block wx:if="{{target.orderbyicon}}"><view class="{{['icon',target.orderbyicon[target.orderby]]}}"></view></block></view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block wx:for="{{goodsList}}" wx:for-item="goods" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','',index]]]]]]]}}" class="product" bindtap="__e"><image mode="widthFix" src="{{goods.imageInfo.whiteImage}}"></image><view class="name">{{goods.skuName}}</view><view class="info"><view class="price">{{goods.priceInfo.price}}</view><view class="slogan">{{(goods.comments||0)+"人评价"}}</view></view></view></block></view><view class="loading-text">{{loadingText}}</view></view></view>